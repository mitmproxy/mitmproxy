{
  "name": "Claude",
  "vendor": "Anthropic",
  "vendor_url": "https://anthropic.com",
  "category": "ai-chat",
  "platforms": [
    "macos",
    "windows"
  ],
  "signatures": {
    "macos": {
      "bundle_id": "com.anthropic.claudefordesktop",
      "team_id": "Q6L2SF6YDW"
    }
  },
  "icon": "claude.png",
  "api_domains": [
    "claude.ai",
    "api.anthropic.com",
    "a-api.anthropic.com"
  ],
  "features": {
    "chat": {
      "type": "chat",
      "name": "Chat Conversation",
      "description": "Chat with Claude AI assistant",
      "patterns": {
        "request": {
          "url": "**/api/organizations/*/chat_conversations/*/completion",
          "method": "POST"
        },
        "response": {
          "url": "**/api/organizations/*/chat_conversations/*",
          "method": "GET",
          "query_params": [
            "tree=True"
          ]
        }
      },
      "parser": {
        "request": {
          "prompt": "prompt",
          "timezone": "timezone",
          "locale": "locale"
        },
        "response": {
          "conversation_id": "uuid",
          "conversation_name": "name",
          "response": "chat_messages[-1].content[0].text",
          "timestamp": "chat_messages[-1].created_at"
        }
      }
    }
  },
  "detection": {
    "subscription": [
      {
        "type": "api_response",
        "url": "**/api/organizations/*/subscription_details",
        "extract": {
          "status": "$.status",
          "billing_interval": "$.billing_interval",
          "next_charge_date": "$.next_charge_date"
        }
      },
      {
        "type": "api_response",
        "url": "**/api/organizations/*",
        "extract": {
          "org_name": "$.name",
          "org_id": "$.uuid"
        }
      }
    ]
  },
  "pricing": {
    "model": [
      "subscription"
    ],
    "tiers": [
      {
        "id": "free",
        "name": "Free",
        "type": "free",
        "monthly_usd": 0
      },
      {
        "id": "pro",
        "name": "Pro",
        "type": "individual",
        "monthly_usd": 20
      },
      {
        "id": "team",
        "name": "Team",
        "type": "team",
        "monthly_usd": 25
      },
      {
        "id": "enterprise",
        "name": "Enterprise",
        "type": "enterprise",
        "monthly_usd": null
      }
    ]
  },
  "_status": "complete",
  "_har_analyzed": "2026-01-13",
  "_notes": "SSE streaming with event types: message_start, content_block_start, content_block_delta, content_block_stop, message_delta, message_limit, message_stop. Response body is base64 encoded in HAR."
}
